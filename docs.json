{
  "$schema": "https://mintlify.com/docs.json",
  "theme": "mint",
  "name": "Influship API",
  "colors": {
    "primary": "#8B5CF6",
    "light": "#A78BFA",
    "dark": "#7C3AED"
  },
  "favicon": "/favicon.svg",
  "navigation": {
    "tabs": [
      {
        "tab": "Quick Start",
        "groups": [
          {
            "group": "Get Started",
            "pages": ["index", "quickstart", "implementation-status"]
          },
          {
            "group": "Authentication",
            "pages": ["guides/authentication"]
          }
        ]
      },
      {
        "tab": "API Reference",
        "groups": [
          {
            "group": "Overview",
            "pages": [
              "api-reference/introduction",
              "api-reference/data-formats"
            ]
          },
          {
            "group": "Endpoints",
            "openapi": "https://api.influship.com/openapi.yaml"
          }
        ]
      },
      {
        "tab": "Examples",
        "groups": [
          {
            "group": "Code Examples",
            "pages": ["examples/javascript", "examples/python", "examples/curl"]
          }
        ]
      },
      {
        "tab": "Support",
        "groups": [
          {
            "group": "Help",
            "pages": [
              "support/faq",
              "support/troubleshooting",
              "support/contact"
            ]
          }
        ]
      }
    ],
    "global": {
      "anchors": [
        {
          "anchor": "API Status",
          "href": "https://status.influship.com",
          "icon": "chart-line"
        },
        {
          "anchor": "Dashboard",
          "href": "https://dashboard.influship.com",
          "icon": "chart-bar"
        },
        {
          "anchor": "Support",
          "href": "mailto:support@influship.com",
          "icon": "envelope"
        }
      ]
    }
  },
  "logo": {
    "light": "/logo/light.svg",
    "dark": "/logo/dark.svg"
  },
  "navbar": {
    "links": [
      {
        "label": "Status",
        "href": "https://status.influship.com"
      },
      {
        "label": "Support",
        "href": "mailto:support@influship.com"
      }
    ],
    "primary": {
      "type": "button",
      "label": "Get API Key",
      "href": "https://dashboard.influship.com"
    }
  },
  "contextual": {
    "options": [
      "copy",
      "view",
      "chatgpt",
      "claude",
      "perplexity",
      "mcp",
      "cursor",
      "vscode"
    ]
  },
  "footer": {
    "socials": {
      "x": "https://x.com/influship",
      "github": "https://github.com/influship",
      "linkedin": "https://linkedin.com/company/influship"
    }
  }
}
